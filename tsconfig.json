{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es5",
		"module": "es2015",
		"moduleResolution": "node",
		"lib": [
			"es2015",
			"es2016",
			"es2017",
			"dom"
		],
		"types": [
			"node"
		],
		"noImplicitReturns": false,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"strict": true,
		"strictNullChecks": true
	},
	"exclude": [
		"node_modules"
	]
}